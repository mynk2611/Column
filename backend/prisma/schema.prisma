
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Column_User {
  id        String @id @default(uuid())
  name      String? 
  email     String @unique
  password  String
  bio       String?  
  column    Column[]
}

model Column {
  id          String @id @default(uuid())
  title       String
  content     String
  published   Boolean
  user        Column_User @relation(fields: [author_id], references: [id])
  author_id   String
}